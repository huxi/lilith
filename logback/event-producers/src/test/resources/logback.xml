<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<param name="Pattern" value="%-5level - %d{HH:mm:ss.SSS} [%thread] - %file:%line - %msg%n%ex{full}" />
		</layout>
	</appender>
	
	<root>
		<level value="DEBUG"/>
		<appender-ref ref="CONSOLE"/>
	</root>
	
	<logger name="class">
		<level value="INFO"/>
	</logger>
</configuration>
